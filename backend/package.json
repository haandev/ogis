{
  "name": "korri-starter",
  "version": "1.0.0",
  "repository": "https://github.com/haandev/korri-starter.git",
  "author": "haandev",
  "scripts": {
    "pre-setup": "npx rimraf ./node_modules/@ooic && npx rimraf ./yarn.lock",
    "setup": "npm run pre-setup && yarn",
    "start": "node build/index.js",
    "start:build": "npm run build && npm run start",
    "copy-core": "npx rimraf ./node_modules/@ooic && npx copy-folder-util npm-local/@ooic node_modules/@ooic  --quiet",
    "dev": "npm run copy-core && npx rimraf ./build-debug && node node_modules/@ooic/scripts/dev.js",
    "debug": "npm run dev",
    "build": "npm run copy-core && node node_modules/@ooic/scripts/build.js",
    "build-debug": "npm run copy-core && node node_modules/@ooic/scripts/build-debug.js"
  },
  "license": "MIT",
  "dependencies": {
    "@ooic/core": "./npm-local/@ooic/core",
    "@ooic/router": "./npm-local/@ooic/router",
    "@ooic/scripts": "./npm-local/@ooic/scripts",
    "@ooic/utils": "./npm-local/@ooic/utils",
    "@sequelize/core": "^7.0.0-alpha.23",
    "@types/asciify": "^1.3.30",
    "@types/chalk": "^2.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/estree": "^0.0.51",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.0.1",
    "firebase-admin": "^12.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "json-schema-faker": "^0.5.0-rcv.46",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^7.5.2",
    "openapi-types": "^12.1.3",
    "swagger-ui-express": "^4.3.0",
    "ts-loader": "^9.2.6",
    "uid2": "^1.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.4",
    "webpack-node-externals": "^3.0.0",
    "zod-to-json-schema": "^3.21.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uid2": "^0.0.0",
    "copy-folder-util": "^0.2.5",
    "fs-extra": "^11.1.0"
  }
}
